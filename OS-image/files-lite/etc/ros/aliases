alias ros-nodes-logs="journalctl -b --no-pager --no-hostname -all -n 1000 --user-unit ros-nodes"
alias ros-nodes-start="systemctl --user start ros-nodes"
alias ros-nodes-restart="systemctl --user restart ros-nodes"
alias ros-nodes-stop="systemctl --user stop ros-nodes"
alias ros-nodes-status="systemctl --user status ros-nodes"
alias ros-nodes-enable="systemctl --user enable --now ros-nodes"
alias ros-nodes-disable="systemctl --user disable --now ros-nodes"
alias uros-agent-logs="journalctl -b --no-pager --no-hostname -all -n 1000 --user-unit uros-agent"
alias uros-agent-start="systemctl --user start uros-agent"
alias uros-agent-restart="systemctl --user restart uros-agent"
alias uros-agent-stop="systemctl --user stop uros-agent"
alias uros-agent-status="systemctl --user status uros-agent"
alias uros-agent-enable="systemctl --user enable --now uros-agent"
alias uros-agent-disable="systemctl --user disable --now uros-agent"
alias ros-logs="journalctl -b --no-pager --no-hostname -all -n 1000 --user-unit ros-nodes --user-unit uros-agent"
alias ros-start="systemctl --user start ros.target"
alias ros-restart="systemctl --user restart ros.target"
alias ros-stop="systemctl --user stop ros.target"
alias ros-status="systemctl --user status ros.target ros-nodes uros-agent"
alias ros-enable="systemctl --user enable --now ros.target"
alias ros-disable="systemctl --user disable --now ros.target"
